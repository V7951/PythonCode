#Ejemplo para break
print("While con la sentencia \n")
contador=0
while contador<10:
    contador +=1

    if contador == 5:
        break

    print("Valor actual de la variable ", contador)

print("Fin del programa break")


#Ejemplo para continue

print("\nWhile con la sentencia \n")
contador=0
while contador<10:
    contador +=1

    if contador == 5:
        continue

    print("Valor actual de la variable ", contador)

print("Fin del programa continue")


